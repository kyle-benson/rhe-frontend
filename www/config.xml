<?xml version='1.0' encoding='utf-8'?>
<widget id="com.feedhenry.apps.rhefrontend" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Red Hat Partner Event</name>
    <description>
        An event agenda application for content delivery.
    </description>
    <author email="msurbey@redhat.com" href="https://ecwpz91.github.io">
        Michael Surbey
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1.2.0" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="res/icon.png" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
</widget>
